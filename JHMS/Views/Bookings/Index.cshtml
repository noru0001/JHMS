@model IEnumerable<JHMS.Models.Booking>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <link href="~/Styles/site.css" rel="stylesheet" />
    <script type="text/javascript>







    </script>


<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TotalAmount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bookFrom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bookTo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.visitorcount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Customer.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TotalAmount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bookFrom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bookTo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.visitorcount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Customer.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
